{
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "extends": [
        "plugin:v3xlabs/recommended"
        // "plugin:node/recommended-module"
    ],
    "ignorePatterns": ["!**/*"],
    "plugins": ["v3xlabs", "node"],
    "env": {
        "node": true
    },
    "rules": {
        "node/file-extension-in-import": ["error", "always"]
    },
    "settings": {}
}
